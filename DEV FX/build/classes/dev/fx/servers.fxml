<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.fx.ServersController">
   <children>
      <Label layoutX="285.0" layoutY="14.0" text="Servers">
         <font>
            <Font size="20.0" />
         </font></Label>
      <TableView fx:id="serversTable" layoutX="45.0" layoutY="156.0" prefHeight="200.0" prefWidth="527.0">
        <columns>
          <TableColumn fx:id="adress" prefWidth="91.0" text="Adress" />
          <TableColumn fx:id="users" prefWidth="73.0" text="Users" />
            <TableColumn fx:id="location" prefWidth="136.0" text="Location" />
            <TableColumn fx:id="maxUsers" prefWidth="101.0" text="Max Users" />
            <TableColumn fx:id="name" prefWidth="125.0" text="Name" />
        </columns>
      </TableView>
      <TextField fx:id="serverJoin" layoutX="122.0" layoutY="434.0" promptText="Type serveradress" />
      <Button layoutX="490.0" layoutY="434.0" mnemonicParsing="false" onAction="#pickServer" text="Join server" />
      <Label fx:id="serverError" layoutX="308.0" layoutY="501.0" text="" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#toMainMenu" text="Back">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
