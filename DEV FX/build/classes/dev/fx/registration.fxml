<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.fx.RegistrationController">
  <children>
      <VBox layoutX="200.0" layoutY="91.0">
         <children>
          <TextField fx:id="name" layoutX="181.0" layoutY="91.0" prefWidth="200.0" promptText="Name" />
          <TextField fx:id="surname" layoutX="181.0" layoutY="133.0" prefWidth="200.0" promptText="Surname" />
          <TextField fx:id="username" layoutX="181.0" layoutY="168.0" prefWidth="200.0" promptText="Username" />
          <PasswordField fx:id="password" prefWidth="200.0" promptText="password" />
         </children>
      </VBox>
    <Button fx:id="registerButton" layoutX="256.0" layoutY="274.0" mnemonicParsing="false" onAction="#checkInput" text="Register" />
    <Label fx:id="messageLabel" layoutX="267.0" layoutY="247.0" text="" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToLogin" text="Back" />
  </children>
</AnchorPane>
